.principal-top{
	height: 75px;
	border-bottom: 1px solid #e0e0e0cf;
}
.circle {
  background-color: #aaa;
  margin-left: 10px;
  width: 60px;
  height: 60px;
  overflow: hidden;
  position: relative;
}

.circle img {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.principal-top h2 {
  position: absolute;
  left: 100px;
  top: 5px;
}
.principal-top .panel-usuario{
  position: absolute;
  right: 50px;
  top: 27px;
  color: #ff4081;
}
.principal-menu{
	position: absolute;
	top: 15px;
	right: 10px;
}
.principal-setting-icon{
	font-size: 36px;
}
.menu{
  width: 80px;
}
.menu button{
  width: 100%;
  padding: 10px 0 10px 0;
  font-size: 10px;
}
.sidenav{
  position: absolute;
  top: 76px;
  bottom: 0;
  left: 0;
  right: 0;
}